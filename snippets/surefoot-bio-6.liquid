<style>
@media only screen and (max-width: 1024px) {
  ul#product-carousels,
  .product-single__mini-slider,
  .product-single__thumbnails {
    display: none !important; }

  #sf-slider {
    position: relative; }
    #sf-slider .MagicToolboxSlide {
      display: block; }
    #sf-slider a.mobile-video {
      padding: 20px 0; }
    #sf-slider .sf-slide img {
      width: 100%; }
    #sf-slider .progress {
      background: #5d5d5d;
      position: absolute;
      height: 3px;
      -webkit-transition: left 0.3s;
      transition: left 0.3s;
      position: relative;
      z-index: 1;
      margin-bottom: -3px; }
    #sf-slider .sf-thumbs {
      background: #f3f3f3;
      font-size: 0;
      margin: 0;
      border-top: 3px solid #d9d9d9;
      text-align: center;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-flex-wrap: nowrap;
      flex-wrap: nowrap;
      overflow: auto;
      -webkit-box-align: center;
      -webkit-align-items: center;
      align-items: center;
      -webkit-overflow-scrolling: touch; }
      #sf-slider .sf-thumbs .sf-slide {
        cursor: pointer;
        margin: 5px;
        width: 62px;
        font-size: 0;
        border: 0;
        height: 62px;
        float: none;
        display: inline-block;
        padding: 0;
        -webkit-flex-shrink: 0;
        flex-shrink: 0;
        -webkit-box-flex: 0;
        -webkit-flex-grow: 0;
        flex-grow: 0; }
        #sf-slider .sf-thumbs .sf-slide.active {
          -webkit-filter: brightness(80%);
          filter: brightness(80%); }
    #sf-slider .flex-direction-nav {
      position: absolute;
      top: 35%;
      left: 0;
      right: 0;
      -webkit-transform: translateY(-100%);
      transform: translateY(-100%);
      display: block !important;
      z-index: 1;
      margin: 0;
      pointer-events: none; }
    #sf-slider .flex-direction-nav li {
      display: inline-block !important; }
    #sf-slider .flex-direction-nav .flex-prev {
      left: 10px;
      -webkit-transform-origin: center center;
      transform-origin: center center;
      -webkit-transform: rotate(180deg);
      transform: rotate(180deg); }
    #sf-slider .flex-direction-nav .flex-next {
      right: 10px; }
    #sf-slider .flex-direction-nav .flex-prev,
    #sf-slider .flex-direction-nav .flex-next {
      pointer-events: all;
      position: absolute;
      top: 0;
      text-decoration: none !important;
      border: 0px solid #9a9a9a !important;
      outline: none !important;
      border-radius: 100%;
      color: #fff !important;
      font-weight: bold;
      padding: 5px;
      background: #35a8a6 url("//cdn-3.convertexperiments.com/uf/10021209/10021350/1547941059angle-right.png") center center no-repeat;
      background-size: 8px;
      width: 20px;
      padding: 15px;
      height: 20px;
      text-indent: 1000px;
      overflow: hidden; }
      #sf-slider .flex-direction-nav .flex-prev:before,
      #sf-slider .flex-direction-nav .flex-next:before {
        display: none; }
}
</style>
