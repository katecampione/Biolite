<div class="mobile-wrapper closed">
 <h2 class="mobile-title">Specs</h2>
 <section id="product-specs" class="mobile-wrapper-content product-specs section product-specs--{{ product.handle | remove: 'biolite-' }}">
   <div class="wrapper">
     <div id="product-specs-new" class="wrapper wrapper--large wrapper--max_height">
    
       <div class="large--one-third grid__item">
         <div class="row-tight">
           <div class="spec-title">Input</div>
           <div class="spec-desc light"><span>100-240 VAC, 50-60 Hz, 0.6A</span></div>
 	
        </div> 
       </div>
       
       <div class="large--one-third grid__item">
         <div class="row-tight">
           <div class="spec-title">Output</div>
           <div class="spec-desc light"><span>12V, 2A</span></div>
         </div>
         
             </div>
 
    <div class="large--one-third grid__item">
         <div class="row-tight">
           <div class="spec-title">Output Connector</div>
           <div class="spec-desc light"><span>4.5mm DC Plug </span></div>
         </div>
         
             </div>
       
          <div class="large--one-third grid__item">
         <div class="row-tight">
           <div class="spec-title">Input Type</div>
           <div class="spec-desc light"><span>Un-grounded Type A outlet (US) </span></div>
         </div>
         
             </div>
     
     </div>
   </div>    
 </section>
</div>
