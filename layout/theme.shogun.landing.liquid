function ValidateEmail(inputText)
{
	var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,14})+$/;
	if(inputText.value.match(mailformat))
	{
		document.getElementById('email-error').innerHTML = '';
      	document.ContactUs.email.focus();
      	return true;
	}
	else
	{
		document.getElementById('email-error').innerHTML = '<span style="color:red;">You have entered an invalid email address!</span>';
      	document.ContactUs.email.focus();
		return false;
	}
}

