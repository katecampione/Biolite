<!-- CSS Convert - Revenue Tracking Goal-->

<script type="text/javascript">
(function() {
  // set DEBUG mode if param `debug` is present in url
  var DEBUG = (window.location.href.indexOf('debug') > 0)
  if (DEBUG) console.log('Convert Revenue goal tracker - intitialized')

  // only run on Checkout Success pages (that end in /thank_you)
  var isCheckoutSuccessPage = !!(window.location.href.indexOf('/thank_you') > 0) 

  
  var $this = this
  /**
   * make sure that Shopify.checkout.total_price and Shopify.checkout.line_items are defined before
   * pushing the goal to Convert.
   */
  function checkForRevenue() {
    if ($this.revenueCheck.count > $this.revenueCheck.max) return $this.revenueCheck.clear()

    // increment our attempt counter
    $this.revenueCheck.count++

    if (isCheckoutSuccessPage && window.Shopify.checkout && (typeof window.Shopify.checkout.total_price !== 'undefined' && typeof window.Shopify.checkout.line_items !== 'undefined')) {
      // clear the interval!
      $this.revenueCheck.clear()
      if (DEBUG) console.log('Convert Revenue goal tracker - ecom data found. pushing events')

      /**
       * Convert.com - Revenue tracking goal
       */

      window._conv_q = window._conv_q || []

      // revenue = total in dollars.cents (e.g. 123.45)
      var convertRevenue = parseFloat(window.Shopify.checkout.total_price)

      // products_cnt = number of products in cart
      var convertProductsCount = window.Shopify.checkout.line_items.reduce(function(count, product) {
        return count += product.quantity
      }, 0)

      // trigger revenue goal
      window._conv_q.push(['pushRevenue', convertRevenue, convertProductsCount, '100217949', 'force_multiple'])
    }
  }

  /**
   * initialize interval check
   */
  this.revenueCheck = {
    max: 100,
    count: 0,
    interval: setInterval(checkForRevenue, 100),
    clear: function() {
      clearInterval(this.interval)
    }
  }
})()
</script>
   
