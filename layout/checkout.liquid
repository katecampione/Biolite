<script type="text/javascript">
/**
 * Google Analytics Enhanced Ecommerce - Checkout Labels
 * - to be fired on all Checkout pages (after GA tag)
 */

(function() {
  /**
   * looks at `Shopify.Checkout` to determine which checkout step we're on, 
   * then pushes that step info to Google Enhanced Ecommerce for Checkout Labeling
   */
  function fireEnhancedEcommerce() {
    var trackerSend = 'send'
    var ShopifyCheckoutstep = Shopify.Checkout.step;

    switch (ShopifyCheckoutstep) {
    case 'contact_information':
      // Step 2: Shipping Address
      ga('require', 'ec');
      ga('ec:setAction', 'checkout', {
        'step': 2,
        'option': 'contact_information'
      });
      ga(trackerSend, 'event', 'Checkout', 'Shipping Address');
      break; 
    case 'shipping_method':
      // Step 3: Shipping Method
      ga('require', 'ec');
      ga('ec:setAction', 'checkout', {
        'step': 3,
        'option': 'shipping_method'
      });
      ga(trackerSend, 'event', 'Checkout', 'Shipping Method');
      break; 
    case 'payment_method':
      // Step 4: Payment Method
      ga('require', 'ec');
      ga('ec:setAction', 'checkout', {
        'step': 4,
        'option': 'payment_method'
      });
      ga(trackerSend, 'event', 'Checkout', 'Payment Method');
      break;
    case 'thank_you':
      // Step 6: Thank You / Confirmation    
      ga('require', 'ec');
      ga('ec:setAction', 'checkout', {
        'step': 5,
        'option': 'thank_you'
      });
      ga(trackerSend, 'event', 'Checkout', 'Thank You');
      break;
    }
  }

  var attempts = 0;
  var MAX_ATTEMPTS = 20;
  var TIMEOUT_INTERVAL = 500;

  /**
   * check for GA functions and make sure the tracker is loaded on the page
   */
  function pollForGA() {
    setTimeout(function(){
      if (window.ga && window.ga.loaded && typeof window.ga.getAll === 'function' && window.ga.getAll().length) {
        // GA has been loaded, proceed with event push
        fireEnhancedEcommerce()

      } else if (attempts < MAX_ATTEMPTS) {
        // GA not loaded yet, but still under max attempts, so keep looking
        attempts++;
        pollForGA();
      }
    }, TIMEOUT_INTERVAL)
  }
  
  pollForGA();
})();
  </script>
   
