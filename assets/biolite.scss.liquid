/**
 * HERO CAROUSEL - ipad/mobile
 */
html.supports-touch {
	.hero-carousel-nav, .flex-control-thumbs {
	  background-color: transparent;
	  border: none;
	  height: 100px;
	  text-align: center;
	  height: 20px;
	  margin-top: -40px;
	}
	.hero-carousel-nav:after, .flex-control-thumbs:after {
	  display: none;
	}
	.hero-carousel-nav li, .flex-control-thumbs li {
	  border: none;
	  background-color: transparent;
	  width: 14px;
	  height: 14px;
	  -webkit-border-radius: 50%;
	  -moz-border-radius: 50%;
	  border-radius: 50%;
	  background-image: none !important;
	  border: 2px solid #fff;
	  float: none;
	  display: inline-block;
	  margin: 0 5px;
	}
	.hero-carousel-nav li:before, .flex-control-thumbs li:before {
	  display: none;
	}
	.hero-carousel-nav li.active > div, .hero-carousel-nav li .flex-control-thumbs li.active > div, .flex-control-thumbs li.active > div, .flex-control-thumbs li .flex-control-thumbs li.active > div {
	  opacity: 0;
	  background-color: transparent;
	}
	.hero-carousel-nav li.active, .flex-control-thumbs li.active {
	  background-color: rgba(255, 255, 255, 0.65);
	}
	.hero-carousel-nav li:first-child,
	.hero-carousel-nav li:last-child, .flex-control-thumbs li:first-child,
	.flex-control-thumbs li:last-child {
	  border: 2px solid #fff;
	}

	.hero.hero--carousel .hero__viewport {
		height: 100vh;
	}

	// Small
	.hero.hero--carousel.hero--carousel-small {
		height: 90vh;

		.hero__viewport { height: 90vh; }
	}

	.hero.hero--carousel.hero--carousel-small .hero-carousel ul.slides > li {
		height: 90vh;
	}

	.hero-carousel-nav li:before, .flex-control-thumbs li:before {
		min-width: 0;
		left: 0;
		background-size: auto;
	}

	.hero--carousel-mission .hero-carousel-nav li, .hero--carousel-mission .flex-control-thumbs li {
		height: 14px;
	}

	.hero-carousel-nav li, .flex-control-thumbs li {
		border: 2px solid #f5f5f5;
	}

